{
    "name":"Duotone",
    "widgetVersion": 2,
    "thumbnail":"images/the-edge-thumbnail.png",
    "templateType":"standard",
    "pageTypes": [ "home", "404" ],
    "screenshot":"images/the-edge-screenshot.jpg",
     "images":[
        "images/gallery/gallery-image--1.jpg",
        "images/gallery/gallery-image--2.jpg",
        "images/gallery/gallery-image--3.jpg",
        "images/gallery/gallery-image--4.jpg",
        "images/gallery/gallery-image--5.jpg",
        "images/gallery/gallery-image--6.jpg",
        "images/gallery/gallery-image--7.jpg",
        "images/gallery/gallery-image--8.jpg",
        "images/gallery/gallery-image--9.jpg"
    ],
    "colorSwatches":
    {
        "Swatch 1": {
            "color1":"#ff31f6",
            "color2":"#00ffba",
            "color3":"#a776ff",
            "color4":"#002e37",
            "color5":"#424d51",
            "color6":"#3d5761",
            "color7":"#ffffff"
        }
    },
    "fontSwatch": {
       "title": "Futura / Playfair Display",
        "font1": {
            "font-family": "\"Futura\", Helvetica, Arial, sans-serif",
            "font-size": "50px",
            "font-weight": "900",
            "line-height": "60px",
            "color": "@color-swatch5",
            "letter-spacing": "-0.03em"
        },
        "font2": {
            "font-family": "\"Futura\", Helvetica, Arial, sans-serif",
            "font-size": "36px",
            "font-weight": "900",
            "line-height": "42px",
            "color": "@color-swatch5",
            "letter-spacing": "-0.03em"
        },
        "font3": {
            "font-family": "\"Futura\", Helvetica, Arial, sans-serif",
            "font-size": "26px",
            "font-weight": "900",
            "line-height": "32px",
            "color": "@color-swatch5",
            "letter-spacing": "-0.03em"
        },
        "font4": {
            "font-family": "\"Futura\", Helvetica, Arial, sans-serif",
            "font-size": "18px",
            "font-weight": "400",
            "line-height": "24px",
            "color": "@color-swatch5",
            "letter-spacing": "0.1em"
        },
        "font5": {
            "font-family": "\"Playfair Display\", Palatino, Georgia, serif",
            "font-size": "18px",
            "font-weight": "400",
            "line-height": "30px",
            "color": "@color-swatch4",
            "letter-spacing": "0.02em",
            "background-color": "@color-swatch3",
            "background-color-hover": "darken(@color-swatch3,5%)"
        },
        "font6": {
            "font-family": "\"Playfair Display\", Palatino, Georgia, serif",
            "font-size": "38px",
            "font-weight": "400",
            "line-height": "48px",
            "color": "@color-swatch4",
            "letter-spacing": "inherit"
        },
        "font7": {
            "font-family": "\"Playfair Display\", Palatino, Georgia, serif",
            "font-size": "20px",
            "font-weight": "400",
            "line-height": "27px",
            "color": "@color-swatch4",
            "letter-spacing": "inherit"
        }
    },
    "styles": {
        "colors": {
            "color__primary": "@color-swatch1",
            "color__secondary": "@color-swatch2",
            "color__tertiary": "@color-swatch3"
			
        },
        "site": {
            "color__body--background": "@color-swatch7",
            "color__container--background": "@color__secondary",
            "color__link--text": "@color__primary",
            "font__companyname--font-family": "@font-swatch2-font-family",
            "font__companyname--font-size": "26px",
            "font__companyname--font-weight": "400",
            "font__companyname--line-height": "32px",
            "font__companyname--letter-spacing": "2px",
            "color__companyname--text": "@color__body--background"
        },
        "typography": {
            "font__h1--font-family": "@font-swatch1-font-family",
            "font__h1--font-size": "@font-swatch1-font-size",
            "font__h1--font-weight": "@font-swatch1-font-weight",
            "font__h1--line-height": "@font-swatch1-line-height",
            "font__h1--letter-spacing": "@font-swatch1-letter-spacing",
            "color__h1--text": "@font-swatch1-color",
            "font__h2--font-family": "@font-swatch2-font-family",
            "font__h2--font-size": "@font-swatch2-font-size",
            "font__h2--font-weight": "@font-swatch2-font-weight",
            "font__h2--line-height": "@font-swatch2-line-height",
            "font__h2--letter-spacing": "@font-swatch2-letter-spacing",
            "color__h2--text": "@font-swatch2-color",
            "font__h3--font-family": "@font-swatch3-font-family",
            "font__h3--font-size": "@font-swatch3-font-size",
            "font__h3--font-weight": "@font-swatch3-font-weight",
            "font__h3--line-height": "@font-swatch3-line-height",
            "font__h3--letter-spacing": "@font-swatch3-letter-spacing",
            "color__h3--text": "@font-swatch3-color",
            "font__h4--font-family": "@font-swatch4-font-family",
            "font__h4--font-size": "@font-swatch4-font-size",
            "font__h4--font-weight": "@font-swatch4-font-weight",
            "font__h4--line-height": "@font-swatch4-line-height",
            "font__h4--letter-spacing": "@font-swatch4-letter-spacing",
            "color__h4--text": "@font-swatch4-color",
            "font__p--font-family": "@font-swatch5-font-family",
            "font__p--font-size": "@font-swatch5-font-size",
            "font__p--font-weight": "@font-swatch5-font-weight",
            "font__p--line-height": "@font-swatch5-line-height",
            "font__p--letter-spacing": "@font-swatch5-letter-spacing",
            "color__p--text": "@font-swatch5-color"
        },
        "header": {
            "color__header--background": "#22242b"
        },
        "navigation": {
			"color__navigation--background": "@color__primary",
            "font__navigation--font-family": "@font-swatch6-font-family",
            "font__navigation--font-size": "12px",
            "font__navigation--font-weight": "900",
            "font__navigation--line-height": "26px",
            "font__navigation--letter-spacing": "1px",
            "color__navigation--text": "#9a9ca5",
            "color__navigation--text--selected": "@color__body--background"
        },
        "form": {
            "color__form-legend--text": "@color__body--background",
            "color__form-legend--background": "@color__primary"
        },
        "button": {
            "color__button--background": "@color__secondary",
            "color__button--text": "#7e8a8e",
            "color__button--background--hover": "darken(@color__button--background, 4%)",
            "color__button--text--hover": "@color__button--text",
            "font__button--font-family": "@font-swatch7-font-family",
            "font__button--font-size": "13px",
            "font__button--font-weight": "700",
            "font__button--line-height": "44px",
            "font__button--letter-spacing": "2px"
        },
        "gallery": {
            "color__gallery-caption--background": "#26292f",
            "color__gallery-title--text": "@color__body--background",
            "color__gallery-description--text": "#bebebe"
        },
        "slideshow": {
            "color__slideshow-caption--background": "#26292f",
            "color__slideshow-title--text": "@color__body--background",
            "color__slideshow-description--text": "#bebebe"
        },
        "feature": {
            "color__feature-content--background": "#26292f",
            "font__feature-headline--font-family": "@font-swatch8-font-family",
            "font__feature-headline--font-size": "72px",
            "font__feature-headline--font-weight": "700",
            "font__feature-headline--line-height": "72px",
            "font__feature-headline--letter-spacing": "6px",
            "color__feature-headline--text": "@color__body--background",
            "font__feature-strapline--font-family": "@font-swatch9-font-family",
            "font__feature-strapline--font-size": "16px",
            "font__feature-strapline--font-weight": "400",
            "font__feature-strapline--line-height": "34px",
            "font__feature-strapline--letter-spacing": "@font-swatch9-letter-spacing",
            "color__feature-strapline--text": "#bebebe",
            "color__feature-button--border": "fade(@color__body--background, 10%)",
            "color__feature-button--background": "transparent",
            "color__feature-button--background--hover": "fade(@color__body--background, 4%)",
            "color__feature-button--text": "@color__body--background"
        },
        "menu": {
            "color__menusections--background": "@color__secondary",
            "color__menu--background": "fade(@color__secondary, 3%)",
            "color__menu-section-header--background": "@color__secondary",
            "color__menu-section-featured--background": "@color__secondary"
        },
        "blog": {
            "color__blogsearch-legend--background": "@color__form-legend--background",
            "color__blogsearch-legend--text": "@color__form-legend--text",
            "color__blogpostlist-article--background": "@color__secondary",
            "color__blogpost-title--background": "@color__secondary",
            "color__blogpost-title--text": "@color__h1--text"
        },
        "ecommerce": {
            "color__ecomproductslist-product-title--text": "@color__p--text",
            "color__ecomproduct-title--text": "@color__h2--text",
            "color__ecomproduct-price--text": "@color__p--text"
        }
    }
}
